{
    "version": 2,
    "functions": {
      "api/create-link.js": {
        "runtime": "nodejs18.x"
      },
      "api/analytics/[id].js": {
        "runtime": "nodejs18.x"
      }
    },
    "rewrites": [
      {
        "source": "/",
        "destination": "/api/index.js"
      },
      {
        "source": "/:id",
        "destination": "/api/redirect.js?id=:id"
      }
    ]
  }